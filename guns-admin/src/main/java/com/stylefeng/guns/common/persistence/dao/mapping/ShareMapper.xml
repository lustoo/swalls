<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.common.persistence.dao.ShareMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.common.persistence.model.Share">
        <id column="id" property="id" />
        <result column="problemName" property="problemName" />
        <result column="problemTime" property="problemTime" />
        <result column="fileName" property="fileName" />
        <result column="filePath" property="filePath" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, problemName, problemTime, fileName, filePath
    </sql>

</mapper>
